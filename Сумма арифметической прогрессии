using System;

namespace Sum {

  class MainClass {

    public static void Main (string[] args) {
    Console.WriteLine("Write N");
      int n=Convert.ToInt32(Console.ReadLine());
      int s=0;
      for (int i=1; i<=n; i++) {
        s+=i;}
      Console.WriteLine($"The sum of the {n} members is {s}");
    }
  }
}
