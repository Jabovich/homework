using System;

namespace triangle {

  class MainClass {

    public static void Main (string[] args) {
      Console.Write("Write M= ");
      int m = Convert.ToInt32(Console.ReadLine());
      for (int i=1; i<m+1; i++)
        {
        for (int j=1; j<i+1; j++)
          {
          Console.Write("*");
          }
        Console.WriteLine();
        }
    }
  }
}
