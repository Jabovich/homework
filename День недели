using System;

namespace day {

  class MainClass {

    public static void Main (string[] args) {
  int a;
      Console.WriteLine("Write number");
      a=Convert.ToInt32(Console.ReadLine());
      switch (a){
          case 1:Console.WriteLine("Monday/Montag");
            break;
          case 2:Console.WriteLine("Tuesday/Dienstag");
            break;
          case 3:Console.WriteLine("Wednesday/Mittwoch");
            break;
          case 4:Console.WriteLine("Thursday/Donnerstag");
            break;
          case 5:Console.WriteLine("Friday/Freitag");
            break;
          case 6:Console.WriteLine("Saturday/Samstag");
            break;
          case 7:Console.WriteLine("Sunday/Sonntag");
            break;
        default:Console.WriteLine("Uncorrect number");
            break;
          }
    }
  }
}
