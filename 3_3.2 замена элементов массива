using System;

namespace HelloWorld {

  class MainClass {
    public static void Main (string[] args) {
    int[] a = new int[8] {0, 2, 6, 15, 16, 40, 30, 11};
    for (int i = 0; i < 8; i++){
      if (15>=a[i]){
        a[i]=a[i]*2;}}
    for (int i = 0; i < 8; i++){
      Console.WriteLine(a[i]);}
    }
  }
}
